
public class Point {
	int x;
	int y;
	
	public Point(int xValue, int yValue)
	{
		x = xValue;
		y = yValue;
	}
	
	public void setX(int value) { x = value; }
	public int getX() { return x; }
	
	public void setY(int value) { y = value; }
	public int getY() { return y; }
//	
	public char displayCharacter() {
	return '~';
//		return isMiss ? '!' : '.';
	}
}
