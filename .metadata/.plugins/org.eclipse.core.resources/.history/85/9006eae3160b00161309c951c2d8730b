
public class MySquare extends GridItem
{
//	
//	height and width, or the circle’s radius.
//	Also give it an appropriate constructor, and implementations of getArea and containsPoint
	
	private int length;
	private int width;
	private int area;
	
	public MySquare(int l, int w)
	{
		length = l;
		width = w;
	}
	
	public int getWidth()
	{
		return width;
	}
		
	public int getLength()
	{
		return length;
	}
	
	public int getArea()
	{
		return length * width;
	}

}
