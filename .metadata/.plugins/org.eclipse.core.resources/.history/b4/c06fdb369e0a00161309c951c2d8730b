
public class GridWriter {
	
	private int x;
	private int y;
	
	public GridWriter(int xCoor, int yCoor)
	{
		x = xCoor;
		y = yCoor;
	}
	
	int getX()
	{
		return x;
	}
	
	void setX(int coor)
	{
		x = coor;
	}
	
	int getY()
	{
		return y;
	}
	
	void setY(int coor)
	{
		y = coor;
	}

	double getArea()
	{
		double area;
		area = x * y;
		
		return area;
	}
	
	boolean containsPoint(int x, int y)
	{
		boolean status = false;
		
		return status;
	}
}
