import java.io.*;

public class TestAverages
{
	public static void main(String[] args) throws IOException
	{
		double average;
		int studentNumber;
		
		TestScoreReader scoreReader = new TestScoreReader("Grades.csv");
		
		while (scoreReader.readNextLine())
		{
		average = scoreReader.getAverage();	
			
		}
		
	}
	
}