public class MyCircle
{
	double x;
	double y;
	double radius;
	
	public void setX(double value)
	{
		x = value;		
	}
	
	
	void setRadius(double value)
	double getRadius()
	double getArea()
	boolean doesOverlap(MyCircle otherCircle)
	
	
	
	public double getX()
	{
		return x;		
	}
	
	public void setY(double value)
	{
		y = value;		
	}
	
	public double getY()
	{
		return y;		
	}
	
	void setRadius(double value)
	{
		radius = value;
	}

	public double getRadius()
	{
		return radius;
	}

}