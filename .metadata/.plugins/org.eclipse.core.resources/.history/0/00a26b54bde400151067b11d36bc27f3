public class Dealer
{
	private int die1Value;
	private int die2Value;
	
	// Constructor
	public Dealer()
	{
		die1Value = 0;
		die2Value = 0;		
	}
	
	public void rollDice()
	{
		final int SIDES = 6;
		
		Die die1 = new Die(SIDES);
		Die die2 = new Die(SIDES);
		
		die1Value = die1.getValue();
		die2Value = die2.getValue();
	}
	
	public String getChoOrHan()
	{
		
		
		
	}

}