public class Rectangle
{
	private double length;
	private double width;
	
	
	// The setLength method stores a value in the length field 
	public void setLength(double myLength)
	{
		length = myLength;
	}
	
	// The setWidth method stores a value in the width field 
	public void setWidth(double myWidth)
	{
		width = myWidth;
	}
	
	
	// The getLength method returns the value stored in the length field.
	public double getLength()
	{
		return length;
	}
	
	
	// The
	public double getWidth()
	{
		return width;
	}
	
	public double getArea()
	{
		return length * width;
	}
}